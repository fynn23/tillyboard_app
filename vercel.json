{
  "redirects": [
    {
      "source": "/en/(.*)",
      "destination": "/$1",
      "permanent": true
    },
    {
      "source": "/de/(.*)",
      "destination": "/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
